{
    "name": "mduplouy/acmewebmanager",
    "version": "0.1",
    "description": "ACME X509 Certificate Web Manager",
    "keywords": [
      "ACME", "CA", "JWS", "Manager"
    ],
    "homepage": "https://github.com/mduplouy/acme-web-manager",
    "license": "LGPLv2+",
    "type": "project",
    "authors": [
        {
            "name": "Mathieu Duplouy",
            "email": "mathieu.duplouy@gmail.com",
            "homepage": "https://github.com/mduplouy"
        }
    ],
    "require": {
        "silex/silex" : "~2.0",
        "twig/twig": ">=1.8,<2.0-dev",
        "symfony/twig-bridge": "~3.1",
        "monolog/monolog": "^1.21",
        "solocreation/acme-php-service-provider": "dev-pimple-3.x",
        "octopuce/acmephpc": "dev-pimple-3.x as v1.0"
